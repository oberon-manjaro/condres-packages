# Maintainer: Condres OS Dev Team (x86_64) <info@codelinsoft.it>

pkgname=nagiosql
pkgver=3.4.0
pkgrel=1
pkgdesc="NagiosQL - nagios configurator"
arch=('i686' 'x86_64')
url="http://www.nagiosql.org/"
license=('BSD')
groups=()
depends=('php' 'mysql' 'nagios')
makedepends=()
optdepends=()
provides=()
conflicts=()
replaces=()
backup=()
options=()
install=
source=("http://downloads.sourceforge.net/project/nagiosql/nagiosql/NagiosQL%203.4.0/$pkgname-3.4.0.tar.gz")
noextract=()
md5sums=('SKIP')

build() {
  _base="$pkgdir/srv/http"
  install -dm755 "${pkgdir}/$_base"
  mv "$srcdir/${pkgname}-${pkgver}" $_base/
}

package() {
  _base="$pkgdir/srv/http"
  install -dm755 "${pkgdir}/$_base"
  mv "$srcdir/${pkgname}-${pkgver}" $_base/
}

